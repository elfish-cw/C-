int[] Fibonacci(int num)
{
    int[] fibo = new int[num];
    fibo[1] = 1;

    for (int i = 2; i < num;i++)
    {
        fibo[i] = fibo[i - 2] + fibo[i - 1]; 
    }
        return fibo;
}

